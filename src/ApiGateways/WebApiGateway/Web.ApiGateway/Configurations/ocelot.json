{
  "Routes": [
    {
      "UseServiceDiscovery": true,
      "ServiceName": "personService",
      "DownstreamPathTemplate": "/api/person/{everything}",
      "DownstreamScheme": "http",

      "UpstreamPathTemplate": "/person/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Delete", "Put" ],
      "UpstreamScheme": "http"
    },
    {
      "UseServiceDiscovery": true,
      "ServiceName": "contactInfoService",
      "DownstreamPathTemplate": "/api/contactinfo/{everything}",
      "DownstreamScheme": "http",

      "UpstreamPathTemplate": "/contactinfo/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Delete", "Put" ],
      "UpstreamScheme": "http"
    }
  ],



  "GlobalConfiguration": {
    //"BaseUrl": "http://webapigateway.api:5000",
    "BaseUrl": "http://localhost:5000",
    "ServiceDiscoveryProvider": {
      //"Host": "c_consul",
      "Host": "http://localhost:5000",
      "Port": 8500,
      "Type": "Consul"
    }
  }


}